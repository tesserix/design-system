{
  "name": "@tesserix/web",
  "version": "1.0.0",
  "description": "Tesserix Design System - Web UI components built with shadcn/ui",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-server": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/themes/default.css",
    "./themes/default": "./dist/themes/default.css",
    "./themes/emerald": "./dist/themes/emerald.css",
    "./themes/sapphire": "./dist/themes/sapphire.css",
    "./themes/rose": "./dist/themes/rose.css",
    "./themes/amber": "./dist/themes/amber.css",
    "./themes/violet": "./dist/themes/violet.css",
    "./themes/teal": "./dist/themes/teal.css",
    "./tailwind": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "tailwind.config.js",
    "README.md"
  ],
  "scripts": {
    "generate:component": "node scripts/generate-component.mjs",
    "generate:themes": "node scripts/generate-themes.mjs",
    "check:bundle-size": "node scripts/check-bundle-size.mjs",
    "perf:report": "npm run build && node scripts/performance-report.mjs",
    "prebuild": "npm run generate:themes",
    "build": "tsup",
    "predev": "npm run generate:themes",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "pretest:storybook": "npm run generate:themes",
    "test:storybook": "vitest run --project storybook",
    "test:storybook:watch": "vitest --project storybook",
    "test:storybook:ui": "vitest --ui --project storybook",
    "test:storybook:coverage": "vitest run --project storybook --coverage",
    "test:storybook:coverage:gate": "npm run test:storybook:coverage && node scripts/check-storybook-coverage.mjs",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "readiness:audit": "node scripts/readiness-audit.mjs",
    "readiness:runtime": "node scripts/check-runtime-warnings.mjs",
    "readiness:gate": "npm run type-check && npm run lint && npm run test:run && npm run build && npm run build-storybook && npm run check:bundle-size",
    "readiness:full": "npm run readiness:gate && npm run readiness:audit && npm run readiness:runtime",
    "prestorybook": "npm run generate:themes",
    "storybook": "storybook dev -p 6006",
    "prebuild-storybook": "npm run generate:themes",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "chromatic:build": "npm run build-storybook && chromatic --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.0"
  },
  "dependencies": {
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@tesserix/tokens": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.13.2",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "chromatic": "^15.1.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.8",
    "tailwindcss": "^4.1.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tesserix/design-system",
    "directory": "packages/web"
  },
  "keywords": [
    "react",
    "components",
    "design-system",
    "shadcn",
    "tailwind",
    "ui",
    "tesserix"
  ],
  "author": "Tesserix",
  "license": "MIT"
}
